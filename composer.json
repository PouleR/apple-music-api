{
    "name": "pouler/apple-music-api",
    "description": "PHP Wrapper for the Apple Music API",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Radt",
            "email": "paulradt@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "keywords": [ "Apple Music", "api" ],
    "homepage": "https://github.com/PouleR/apple-music-api",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^12",
        "squizlabs/php_codesniffer": "^4"
    },
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "symfony/http-client": "^6|^7|^8",
        "web-token/jwt-library": "^3|^4"
    },
    "autoload": {
        "psr-4": {
            "PouleR\\AppleMusicAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PouleR\\AppleMusicAPI\\Tests\\": "tests/"
        }
    }
}
